{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Fructu",
  "colors": {
    "primary": "#1443FF",
    "light": "#1443FF",
    "dark": "#2062FA"
  },
  "appearance": {
    "default": "light",
    "strict": false
  },
  "favicon": {
    "light": "/images/FructuLightMode.svg",
    "dark": "/images/FructuDarkMode.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "icon": "square-terminal",
        "openapi": {
          "source": "api-reference/OpenAPI.json",
          "directory": "api-reference"
        },
        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api-reference/introduction",
              "api-reference/auth"
            ]
          }
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "simple"
    },
    "mdx": {
      "server": "https://api.fructu.co",
      "auth": {
        "method": "bearer"
      }
    }
},
  "footer": {
    "socials": {
    }
  },
  "logo": {
    "light": "/images/FructuLightMode2.png",
    "dark": "/images/FructuDarkMode2.png",
    "href": "https://fructu.co/"
  },
  "background": {
    "decoration": "grid",
    "color": {
      "dark": "#121212"
    }
  },
  "fonts": {
    "family": "Poppins",
    "weight": 400,
    "body": {
      "family": "Inter",
      "weight": 400
    }
  }
}